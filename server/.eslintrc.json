{
    "env": {
        "browser": false,
        "es2021": true,
        "node": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "space-before-function-paren": 0,
        "semi-style": [
            "error",
            "last"
        ]
    }
}